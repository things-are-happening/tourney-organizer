div(class='flex space-between page-header container')
    ul(class='nav nav-tabs')
        li(class='active')
            a(href="#1" data-toggle="tab") Round 1
        li
            a(href="#2" data-toggle="tab") Round 2
        li
            a(href="#3" data-toggle="tab") Quarter Finals
        li
            a(href="#4" data-toggle="tab") Semi Finals
        li
            a(href="#5" data-toggle="tab") Finals
div(class='tab-content')			
    div(class='container-fluid view-body tab-pane active' id='1' role='tabpanel')
        div(class='col-md-3 col-sm-6' ng-repeat='match in matches')
            a(href='http://google.com')
                div(class='match-panel')
                    div(class='team-names')
                        div(class='flex direction-row space-between')
                            h4 {{match.teamOne}}:
                            div(class='scorePosition')
                                h4 {{match.teamOneScore}}
                        div(class='flex direction-row space-between')
                            h4 {{match.teamTwo}}:
                            div
                                h4 {{match.teamTwoScore}}
                        div(class='flex space-around match-panel-date')
                            {{match.date}}, {{match.time}}

    div(class='container-fluid view-body tab-pane' id='2' role='tabpanel')
        div(class='col-md-3 col-sm-6' ng-repeat='match in matches')
            a(href='http://google.com')
                div(class='match-panel')
                    div(class='team-names')
                        div(class='flex direction-row space-between')
                            h4 {{match.teamOne}}:
                            div(class='scorePosition')
                                h4 SCORE:{{match.teamOneScore}} yo
                        div(class='flex direction-row space-between')
                            h4 {{match.teamTwo}}:
                            div
                                h4 {{match.teamTwoScore}}
                        div(class='flex space-around match-panel-date')
                            {{match.date}}, {{match.time}}		



